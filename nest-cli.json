{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/kv-blog/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/kv-blog/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/kv-blog",
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "kv-admin": {
      "type": "application",
      "root": "apps/kv-admin",
      "entryFile": "main",
      "sourceRoot": "apps/kv-admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/kv-admin/tsconfig.app.json"
      }
    },
    "kv-blog": {
      "type": "application",
      "root": "apps/kv-blog",
      "entryFile": "main",
      "sourceRoot": "apps/kv-blog/src",
      "compilerOptions": {
        "tsConfigPath": "apps/kv-blog/tsconfig.app.json"
      }
    },
    "services": {
      "type": "library",
      "root": "libs/services",
      "entryFile": "index",
      "sourceRoot": "libs/services/src",
      "compilerOptions": {
        "tsConfigPath": "libs/services/tsconfig.lib.json"
      }
    }
  }
}